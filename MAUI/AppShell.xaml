<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUI"
    Shell.FlyoutBehavior="Disabled"
    xmlns:views="clr-namespace:MAUI.Views"
    Title="MAUI">

    <ShellContent Title="Listado Personas" Route="listadoPersonas" ContentTemplate="{DataTemplate views:ListadoPersonas}" />
    <ShellContent Title="Editar Persona" Route="editarPersona" ContentTemplate="{DataTemplate views:EditarPersona}" />


</Shell>
